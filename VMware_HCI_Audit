                            @{N="SecondHost";E={resolv $DCSSERVER ID $_.SecondHostId Caption}},
                            @{N="BackupHost";E={resolv $DCSSERVER ID $_.BackupHostId Caption}},
                            @{N="PreferredServer";E={resolv $DCSSERVER ID $_.PreferredServer Caption}},
                            @{N="StorageProfile";E={resolv $DCSPROFILE ID $_.StorageProfileId Caption}},
                            @{N="CDP";E={ if ( (resolv $DCSLOGICALDISK VirtualDiskId $_.ID StreamState) -match "Active") {"true"} else {"false"} }},
                            WriteThrough,
                            IsServed,
                            MirrorTrunkMappingEnabled,
                            ResiliencyEnabled,
                            EncryptionEnabled,
                            CompressionEnabled,
                            DeduplicationEnabled,
                            @{N="Optimization Policy";E={$_.StaleDataEnabled}},
                            @{N="Optimization Policy type";E={switch ($_.StaleDataWeight){
                                    32 {"Lazy"}
                                    64 {"Normal"}
                                    96 {"Aggressive"}
                                    }
                                }}|
                            sort Caption,FirstHost ) 
                            }

                        New-HTMLSection -HeaderText "Logical disks details" -CanCollapse -Collapsed -Direction row {
                        #$i=0
                        #$i++
                        $DCSLOGICALDISK|?{$_.ServerHostId -eq $SDSID  -and $_.StreamDiskDataProtectionRole -ne "HistoryLog"}|sort -Property StorageName| % {
                                $global:DCSLOGICALDISK_=$_
