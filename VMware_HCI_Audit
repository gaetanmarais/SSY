                                }
                            New-HTMLSection -HeaderText "EventLogs - Last reboots & crashs" -CanCollapse -Collapsed -Direction row -HeaderBackGroundColor grey {
                                zeTable $($OSDETAIL.Windows.EventLog.Reboot_and_Crash.ChildNodes |select TimeGenerated,EntryType,Source,InstanceId,Message|sort -Property TimeGenerated) 
                                }
                            New-HTMLSection -HeaderText "EventLogs - last 100 errors" -CanCollapse -Collapsed -Direction row -HeaderBackGroundColor grey {
                                zeTable $($OSDETAIL.Windows.EventLog.Last100.ChildNodes|select TimeGenerated,EntryType,Source,InstanceId,Message|sort -Property TimeGenerated) 
                                }
                            }
