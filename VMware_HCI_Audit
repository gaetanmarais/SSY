    $global:DCSISCSI=$DCSOBJECTMODEL.DataRepository.ServeriScsiPortData
    $global:DCSVIMHOST=$DCSOBJECTMODEL.DataRepository.VimHostData
    $global:DCSVIRTUALDISK=$DCSOBJECTMODEL.DataRepository.VirtualDiskData|?{$_.subtype -ne "Trunk"}
    $global:DCSLOGICALDISK=$DCSOBJECTMODEL.DataRepository.StreamLogicalDiskData
    $global:DCSLOGICALUNIT=$DCSOBJECTMODEL.DataRepository.VirtualLogicalUnitData
    $global:DCSPROFILE=$DCSOBJECTMODEL.DataRepository.StorageProfileData
