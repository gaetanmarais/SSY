        }
    catch { 
        Write-host "DataCore cmdlets for Windows powershell is not installed yet, please installed it and restart the script later." -ForegroundColor Red
        Write-host "         DataCore cmdlets could be downloaded on the DataCore support website." -ForegroundColor Red
