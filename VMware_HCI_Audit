                                        @{N="Pool";E={resolv $DCSPOOL ID $_.PoolId Caption}},
                                        DiskRole,IsMapped,ClientAccessRights,
                                        @{N="TierAffinity";E={$_.TierAffinity.int -join(",")}},
                                        SequentialStorage,
                                        @{N="CDP";E={if ($_.StreamState -eq "Active") {"true"} else {"false"}}}
                                        ) 
                                    zetable $(($DCSLOGICALDISK_| select @{N="Mapping";E={((resolv $DCSLOGICALUNIT LogicalDiskId $_.id Caption)|sort)}}).mapping -split(",")) 
                                    }
                                }
                        }
                        New-HTMLSection -HeaderText "CDPs" -CanCollapse -Collapsed -Direction row {
                        $CDP_=($DCSLOGICALDISK|?{$_.ServerHostId -eq $SDSID -and $_.StreamDiskDataProtectionRole -in @("BaseDisk")})
                        zetable $($CDP_ |Select StorageName,
                            Diskstatus,
                            @{N="Size Gb";E={[Math]::Round($_.size.value/1Gb)}},@{N="Pool";E={resolv $DCSPOOL ID $_.PoolId Caption}},
                            DiskRole,
                            IsMapped,
                            ClientAccessRights,
                            @{N="TierAffinity";E={$_.TierAffinity.int -join(",")}},
                            @{N="Used";E={if ($_.caption -notlike "*@*"){ [Math]::Round($PERFORMANCEDATA[$_.id].perf.StreamBytesUsed/1GB).ToString() + "GB"}}},
                            @{N="Allocated";E={if ($_.caption -notlike "*@*"){ [Math]::Round($PERFORMANCEDATA[$_.id].perf.StreamBytesAllocated/1GB).ToString() + "GB"}}},
                            @{N="Max";E={if ($_.caption -notlike "*@*"){ ([Math]::Round($_.StreamSize.value)/1GB).toString()+"GB" }}},
                            @{N="RetentionPeriod";E={if ($_.caption -notlike "*@*")
                                    {(new-timespan -Seconds ($PERFORMANCEDATA[$_.id].perf.RetentionPeriod/1000))}
                                else
                                    {resolv $dcsrollback DestinationLogicalDiskId $_.ID RestorePoint }
                                    }}|
                            sort -Property StorageName)  
                            
                        $ROLLBACK_=$DCSROLLBACK|?{$_.SourceLogicalDiskId -in $CDP_.ID}
                        if ( $ROLLBACK_) {
                            zeTable  $($ROLLBACK_|select @{N="VDisk";E={resolv $DCSVIRTUALDISK ID $(resolv $DCSLOGICALDISK ID $_.SourceLogicalDiskId VirtualDiskId) Caption}},
                                Caption,
                                @{N="Type";E={if ($_.AutoExpire -eq "true") {"Expire"} else {"Persistent"}}},
                                State,
                                compressionEnabled
                            )}
                        }
                        New-HTMLSection -HeaderText "Snapshots" -CanCollapse -Collapsed {
                        zetable $($DCSSNAPSHOT|select @{N="Vdisk";E={Resolv $DCSLOGICALDISK ID $_.SourceLogicalDiskId StorageName }},
                            Caption,@{N="Pool";E={resolv $DCSPOOL ID (resolv $DCSLOGICALDISK ID $_.SourceLogicalDiskId PoolId) Caption}},
                            Type,
                            State,
                            CompressionEnabled,
                            @{N="MigrationPercentage";E={$PERFORMANCEDATA[$_.ID].perf.MigrationPercentage}},
                            @{N="DifferencePercentage";E={$PERFORMANCEDATA[$_.ID].perf.DifferencePercentage}},
                            @{N="GBMigrated";E={($PERFORMANCEDATA[$_.ID].perf.TotalBytesMigrated)/1GB}} | sort ) 
                            }
                        
                }                    # Close THMLTab vDisks
            }                                              #close for each SDS server
         }           #close SDS HTMLTabPanel
        }                                  #close SANSYmphony TAB




    #  VMWARE
    #
    #
    ###############################################################
    if ( ${DCSVCENTERHOSTNAME} ) {
    write-host "VMware section"            
    Tab -Name "VMware" -IconSolid check-circle {
    
        New-HTMLTabPanel -Orientation vertical -TransitionAnimation fade -Theme brick   { 
        $global:VMHOST=get-vmhost
        $global:VM=Get-VM
        $global:datastore=Get-Datastore
        $global:devices=Get-VMHostHba|%{Get-ScsiLun -Hba $_} #| Select-Object CanonicalName,VMhost,RuntimeName,CanonicalName,Vendor,Model,CapacityGB,MultipathPolicy,CommandsToSwitchPath,BlocksToSwitchPath,IsLocal,IsSSD
        $global:devicespaths=$devices|get-scsilunpath
        $global:vmiscsi=Get-VMHostHba -Type IScsi

        $global:vmportgroup=Get-VirtualPortGroup
        $global:vmswitch=Get-VirtualSwitch
        $global:vmnetwork=Get-VirtualNetwork
        $global:vmnetworkadapters=$VM|get-networkadapter

        $global:devicedatastore=Get-Datastore | ? {$_.ExtensionData.Info.GetType().Name -eq "VmfsDatastoreInfo"} | % {
            if ($_)
                {
                 $ZeDatastore = $_
                 $ZeDatastore.ExtensionData.Info.Vmfs.Extent |
                 Select-Object -Property @{Name="Name";Expression={$ZeDatastore.Name}},DiskName
                }
        }

        #Get-StatType -Entity $VMHOST_
         


        # VMware HOSTS
        ####################################################
            $VMHOST|sort|%{
                $global:VMHOST_=$_
                $global:VMHOSTNAME=$VMHOST_.name
                New-HTMLTab -Name $VMHOST_.name -TextTransform capitalize   -TextSize 20 {

                    
                    $global:VMNTP=$VMHOST_ | Select-Object Name, 
                        @{N=NTPServiceRunning;E={($_ | Get-VmHostService | Where-Object {$_.key-eq ntpd}).Running}}, 
                        @{N=StartupPolicy;E={($_ | Get-VmHostService | Where-Object {$_.key-eq ntpd}).Policy}}, 
                        @{N=NTPServers;E={$_ | Get-VMHostNtpServer}}, 
                        @{N="Date&Time";E={(get-view $_.ExtensionData.configManager.DateTimeSystem).QueryDateTime()}}
                    $global:ESXCLI=($VMHOST_|get-esxcli -v2)
                    $global:VMSATP=$esxcli.storage.nmp.satp.rule.list.invoke()|?{$_.Vendor -eq "DataCore"}
                    

                    #write-host "Gathering VMHOST $VMHOST_ data (CPU, RAM, DISK...)"

                    New-HTMLSection -HeaderText "ESX details" -CanCollapse  {
                        zetable $($VMHOST_|select Name,
                            Version,
                            Manufacturer,
                            Model,
                            NumCpu,
                            ProcessorType,
                            HyperthreadingActive,
                            MaxEVCMode,
                            CpuTotalMhz,
                            CpuUsageMhz,
                            @{N="MemoryTotalGB";E={[Math]::Round($_.MemoryTotalGB,0)}},
                            @{N="MemoryUsageGB";E={[Math]::Round($_.MemoryUsageGB,0)}}) 
                        } 
                    
                    #write-host "NTP"                                                      
                    New-HTMLSection -HeaderText "NTP settings" -CanCollapse -Collapsed { zetable $($VMNTP ) }
                    
                       
                    #write-host "PowerPolicy"                   
                    New-HTMLSection -HeaderText "PowerPolicy settings" -CanCollapse -Collapsed {
                        zetable $($VMHOST_ | Select @{ N="CurrentPolicy"; E={$_.ExtensionData.config.PowerSystemInfo.CurrentPolicy.ShortName}},
                            @{ N="CurrentPolicyKey"; E={$_.ExtensionData.config.PowerSystemInfo.CurrentPolicy.Key}},
                            @{ N="AvailablePolicies"; E={$_.ExtensionData.config.PowerSystemCapability.AvailablePolicy.ShortName}} )
                        }     
                        
                                         
                    #write-host "Advanced settings"
                    New-HTMLSection -HeaderText "Advanced settings" -CanCollapse -Collapsed  {
                        zetable $($ESXCLI.system.settings.advanced.list.Invoke()|select Path,
                            Description,
                            @{N="Changed";E={if ($_.DefaultIntValue -ne $_.IntValue){"True"}}},
                            IntValue,
                            DefaultIntValue,
                            MinValue,
                            MaxValue|sort -Property Path) 
                        }
                        
                                              
                    #write-host "iSCSI"
                    New-HTMLSection -HeaderText "iSCSI" -CanCollapse -Collapsed -Direction row {
                        zetable $($VMiSCSI | ? {$_.vmhost -eq $VMHOST_} | select Name,IscsiName,IscsiAlias   ) 
                        zetable $(($VMiSCSI | ? {$_.vmhost -eq $VMHOST_}).extensiondata.ConfiguredStaticTarget|select Parent,iScsiName,DiscoveryMethod| sort -Property iScsiName)  
                        zetable $(($VMiSCSI | ? {$_.vmhost -eq $VMHOST_}).extensiondata.advancedoptions | select Key,Value,IsInherited ) 
                        }
                         
                    #write-host "Disks devices"
                    New-HTMLSection -HeaderText "Disks Devices" -CanCollapse -Collapsed -Direction row {
                        zetable $($VMSATP|select Name,Vendor,Model,ClaimOptions,PSPOptions,Options) 

                        $Devices|?{$_.vmhost -eq $VMHOST_}|sort|% {
                            $devices_=$_
                            New-HTMLSection -HeaderText "$_.CanonicalName ($(resolv $devicedatastore Diskname $_.CanonicalName name))" -CanCollapse -Collapsed -Direction row -HeaderBackGroundColor grey {
                                zetable $($Devices_ | Select-Object VMhost,
                                    RuntimeName,
                                    CanonicalName,
                                    @{N="DataStore";E={resolv $devicedatastore Diskname $_.CanonicalName name}},
                                    Vendor,
                                    Model,
                                    CapacityGB,
                                    MultipathPolicy,
                                    CommandsToSwitchPath,
                                    BlocksToSwitchPath,
                                    IsLocal,
                                    IsSSD ) 
                                zetable $($devicespaths |? {$_.ScsiLunId -eq $devices_.id}| Select-Object Name,
                                    SanId,
                                    State,
                                    @{N="IsWorking";E={$_.extensiondata.isworkingpath}},
                                    Prefered | sort -Property State,SanId )
                                    }
                        }
                    }    
                    
                                      
                                                            

                    New-HTMLSection -HeaderText "DataStores" -CanCollapse -Collapsed -Direction row {
                        
                        zetable $($VMSATP )  
                        $VMHOST_|Get-Datastore |sort -Property Name |% {
                            $global:DATASTORE_=$_
                            New-HTMLSection -HeaderText $DATASTORE_.Name -CanCollapse -Collapsed -HeaderBackGroundColor grey -Direction row {
                                zetable $($DATASTORE_ |select Name,
                                    Id,
                                    State,
                                    Type,
                                    FileSystemVersion,
                                    @{N="CapacityGB";E={[math]::Round($_.CapacityGB)}},
                                    @{N="FreeSpaceGB";E={[math]::Round($_.FreeSpaceGB)}}) 
                                
                                $VMDEVICES=$esxcli.storage.core.device.list.invoke() | ? {$_.DisplayName -eq "$DATASTORE_"}|select DisplayName,Device,Status,VAAIStatus,MultipathPlugin,IsVVOLPE,IsSSD,IsRDMCapable,IsSharedClusterwide,Vendor,Model
                                zetable $($VMDEVICES ) 

                                $VMDEVICEPATHS=$esxcli.storage.core.path.list.invoke() | ?{$_.DeviceDisplayName -eq "$DATASTORE_" }|select Transport,Adapter,TargetIdentifier,LUN,MaximumIOSize,State
                                zetable $($VMDEVICEPATHS ) 
                            }
                        }
                    }

                    New-HTMLSection -HeaderText "Network Details" -CanCollapse -Collapsed {
                        zetable $($vmportgroup| ?{ $_.VMHostId -eq $VMHOST_.id}|  select Name,
                            VirtualswitchName,
                            Vlanid,
                            @{N="vm";E={resolv $vmnetworkadapters MacAddress $_.port.mac Parent}},
                            @{N="MTU";E={Resolv2 $vmswitch Name $_.VirtualSwitchName VMHostId $_.VMHostId MTU}},
                            @{N="NICs";E={Resolv2 $vmswitch Name $_.VirtualSwitchName VMHostId $_.VMHostId NIC}}|sort -Property VirtualSwitchName,Name ) 
                            }        

                    New-HTMLSection -HeaderText "Vibs" -CanCollapse -Collapsed {
                        zetable $($esxcli.software.vib.list.invoke()|select Name,
                            Vendor,
                            Version,
                            CreationDate,
                            InstallDate,
                            AcceptanceLevel|sort -Property InstallDate -Descending )  
                        } 
                        
                        
                    New-HTMLSection -HeaderText "Graphs" -CanCollapse -Collapsed -Direction row {     
                        
                        $statsType=@("cpu.usage.average","cpu.ready.summation","mem.usage.average","mem.overhead.average")
                        $days=14
                        $stats = Get-Stat -Entity $VMHOST_ -start (get-date).AddDays(-${days}) -Finish (Get-Date) -MaxSamples $(${days}*24*20) -stat $statsType                    
                        $statsType | % {
                            $stattype=$_
                            $data = $stats|?{$_.metricId -eq $stattype}
                                New-HTMLSection -BackgroundColor white {
                                    New-HTMLChart -Title $stattype -TitleAlignment center   {

                                    New-ChartAxisX -Name $data.TimeStamp 
                            
                                    New-ChartLine -Name ($data.unit | select -Unique)  -Value $data.Value -Color blue -Width 1 -Curve smooth 
                                    New-ChartDataLabel -TextAnchor end
                            

                            
                                }
                                }
                            }
                       

                    }    
                    
                    
                                                                                        
               }
            }




        # SDS VMs
        ####################################################
            $VM|?{ $_.name -in $DCSSERVER.caption}|sort | % {
                $SDSVM=$_
                $VMDATA=$VM|?{$_.name -eq $SDSVM}|Select-Object Name,VMhost,PowerState,Guest,NumCPU,CoresPerSocket,@{N='CPUAllocation';E={$_.ExtensionData.ResourceConfig.CPUAllocation.Reservation}},MemoryGB,@{N='MemoryAllocation';E={$_.ExtensionData.ResourceConfig.MemoryAllocation.Reservation}},@{N='MemoryReservationLockedToMax';E={$_.ExtensionData.Config.MemoryReservationLockedToMax}},@{N='LatencySensitivity';E={$_.ExtensionData.Config.LatencySensitivity.level}},HardwareVersion,@{N='OverallStatus';E={$_.ExtensionData.OverallStatus}},@{N='SyncTimeWithHost';E={$_.ExtensionData.Config.Tools.SyncTimeWithHost}},@{N='ChangeVersion';E={$_.ExtensionData.Config.ChangeVersion}}
                $VMNETWORK=Get-NetworkAdapter -vm $SDSVM| select Name,NetworkName,MacAddress,Type,ConnectionState
                $VMHARDDISK=Get-HardDisk -vm $SDSVM | select Name,CapacityGB,DiskType,Persistence,StorageFormat,ScsiCanonicalName
                $VMPASSTHROUGH=Get-PassthroughDevice -VM $SDSVM -ErrorAction SilentlyContinue |select Name,State,VendorName |Out-Null
                New-HTMLTab -Name $SDSVM.Name -TextTransform capitalize   -TextSize 20 {
                    New-HTMLSection -HeaderText "DataCore server details" {
                        zetable $VMDATA
                    }
                    New-HTMLSection -HeaderText "DataCore server Networking" -CanCollapse -Collapsed {
                        zetable $VMNETWORK
                    }
                    New-HTMLSection -HeaderText "DataCore server Storage" -CanCollapse -Collapsed  {
                        zetable $VMHARDDISK
                    }
                    New-HTMLSection -HeaderText "DataCore server Passtrough" -CanCollapse -Collapsed  {
                        zetable $VMPASSTHROUGH
                    }                      
                }
          }
    



        # VMware DataStores
        ####################################################

                New-HTMLTab -Name "Datastore details" -TextTransform capitalize   -TextSize 20 {
                    New-HTMLSection -HeaderText "Datastore details" -CanCollapse  {
                        zetable $($datastore|select Name,
                            Type,
                            FileSystemVersion,
                            @{N="Capacity";E={[math]::Round($_.CapacityGB)}},
                            @{N="Free";E={[math]::Round($_.FreeSpaceGB)}},
                            @{N="% Available";E={[math]::Round($_.FreeSpaceGB/$_.CapacityGB*100)}},
                            Accessible,
                            State,
                            @{N="Hosts";E={($_.ExtensionData.Host.key.value|%{resolvlike $VMHOST ID $_ name}|sort) -join(" ")}},
                            StorageIOControlEnabled|sort -Property Name)
                    }  
                }
    


        # VMware VMs
        ####################################################

        $VMTABLE=($VM |? {$_.PowerState -eq "PoweredOn"} |select VMhost,name,MemoryGB,@{N="CPU";E={$_.NumCpu*$_.CoresPerSocket}} |sort -Property Name|
            Group-Object VMhost | 
            select Name,
            @{N="#VMs";E={ $_.count }},
            @{N="CPU (min)";E={ ($_.group | Measure-Object 'CPU' -Minimum).Minimum}},
            @{N="CPU (max)";E={ ($_.group | Measure-Object 'CPU' -Maximum).Maximum}},
            @{N="CPU (avg)";E={ [Math]::Round(($_.group | Measure-Object 'CPU' -Average).Average)}},
            @{N="CPU (sum)";E={ ($_.group | Measure-Object 'CPU' -Sum).sum}},
            @{N="RAM GB(min)";E={ ($_.group | Measure-Object 'MemoryGB' -Minimum).Minimum}},
            @{N="RAM GB(max)";E={ ($_.group | Measure-Object 'MemoryGB' -Maximum).Maximum}},
            @{N="RAM GB(avg)";E={ [Math]::Round(($_.group | Measure-Object 'MemoryGB' -Average).Average)}},
            @{N="RAM GB(sum)";E={ ($_.group | Measure-Object 'MemoryGB' -Sum).sum}})
        
        
        New-HTMLTab -Name "VMs details" -TextTransform capitalize   -TextSize 20{

                New-HTMLPanel -Invisible  {
                zetable $($VMTABLE)
                New-HTMLSection -CanCollapse -Collapsed {
                    New-HTMLChart -Title "#VMs by host" -TitleAlignment center -TitleColor White {
                    $VMTABLE|%{
                        ChartBar -Name $_.Name -Value $_.'#VMs' 
                        ChartBarOptions -Vertical bar 
                        ChartLegend -Names "# of VMs by host" -UseSeriesColors 
                        
                        

                        }
                }
                    New-HTMLChart -Title "CPU by host" -TitleAlignment center -TitleColor White{
                    $VMTABLE|%{
                        ChartBar -Name $_.Name -Value $_.'CPU (min)',$_.'CPU (max)',$_.'CPU (avg)',$_.'CPU (sum)'
                        ChartBarOptions -Vertical bar
                        ChartLegend -Names "Min","Max","Avg","Sum" -UseSeriesColors -LegendPosition right 

                        }
                }
                    New-HTMLChart -Title "Memory by host" -TitleAlignment center -TitleColor White{
                    $VMTABLE|%{
                        ChartBar -Name $_.Name -Value $_.'RAM GB(min)',$_.'RAM GB(max)',$_.'RAM GB(avg)',$_.'RAM GB(sum)' 
                        ChartBarOptions -Vertical bar
                        ChartLegend -Names "Min GB","Max GB","Avg GB","Sum GB" -UseSeriesColors -LegendPosition right
                        

                        }
                }

                    }
                }

         $VM|sort -Property name |%{
            $VM_=$_
            

            
            New-HTMLSection -Name $VM_.Name -CanCollapse -Collapsed -Direction row {
                zetable $($VM_ | Select-Object Name,
                    PowerState,
                    @{N="AutoStart";E={($_|Get-VMStartPolicy).startaction}},
                    NumCPU,
                    CoresPerSocket,
                    @{N='CPUAllocation';E={$_.ExtensionData.ResourceConfig.CPUAllocation.Reservation}},
                    MemoryGB,
                    @{N='MemoryAllocation';E={$_.ExtensionData.ResourceConfig.MemoryAllocation.Reservation}},
                    @{N='MemoryReservationLockedToMax';E={$_.ExtensionData.Config.MemoryReservationLockedToMax}},
                    @{N='LatencySensitivity';E={$_.ExtensionData.Config.LatencySensitivity.level}},
                    VMHost,GuestId,
                    @{N="UsedSpaceGB";E={[math]::Round($_.UsedSpaceGB)}},
                    @{N="ProvisionedSpaceGB";E={[math]::Round($_.ProvisionedSpaceGB)}},
                    @{N="Datastore";E={resolv $DATASTORE ID $_.DatastoreIdList name}},
                    @{N="SwapLocation";E={$_.ExtensionData.Config.SwapPlacement}},
                    @{N="GuestHeartbeatStatus";E={$_.extensiondata.GuestHeartbeatStatus}},
                    @{N="ToolsStatus";E={$_.ExtensionData.Guest.ToolsStatus}},
                    @{N="ToolsVersionStatus";E={$_.ExtensionData.Guest.ToolsVersionStatus}},
                    @{N="ToolsVersion";E={$_.ExtensionData.Guest.ToolsVersion}},
                    @{N="HwVersion";E={$_.ExtensionData.Guest.HwVersion}},
                    TriggeredAlarmState)

                $statsType=@("cpu.usage.average","cpu.ready.summation","mem.usage.average","disk.usage.average","disk.maxTotalLatency.latest")
                $days=14
                $stats = Get-Stat -Entity $VM_ -start (get-date).AddDays(-${days}) -Finish (Get-Date) -MaxSamples $(${days}*24*20) -stat $statsType                    
                $statsType | % {
                    $stattype=$_
                    $data = $stats|?{$_.metricId -eq $stattype}
                        New-HTMLSection -BackgroundColor white {
                            New-HTMLChart -Title $stattype -TitleAlignment center   {

                            New-ChartAxisX -Name $data.TimeStamp 
                            
                            New-ChartLine -Name ($data.unit | select -Unique)  -Value $data.Value -Color blue -Width 1 -Curve smooth 
                            New-ChartDataLabel -TextAnchor end
                            

                            
                        }
                        }
                    }

                zeTable $( $VM_.ExtensionData.config.extraConfig ) 
                }
            }




    }
        
 
    
 


} #Close VMWARE TablPanel

    }  #Close VMware TAB

    }   #Close VMware check section


####Best Practices
####################################


    if ( ! (get-item "${SCRIPTPATH}\bestpractices.txt" -ErrorAction SilentlyContinue) ) {



$BESTPRACTICESTXT=@'
#######################################################
#  Audit BEst practice file
#
# [xxxxxxxxxx]    Audit TAB name
# {yyyyyyyyyy}    TAB section
# <x>             red Asterix icon (*)
# <!>             red esclamation
# <url: https://datacore.com/xxxx)  url link
# <size: 30>      change the size of the line
# <color: green>  change the color of the line


[DataCore Servers]
{Operating System}
<!> test
{Hardware}
kmlkmglk <url:https://toto.com/tamere> gfsdgsfdgsf

[VMware]
{DataCore VMs}
To avoid leak of ressources for the SSY VMs and generate latencies in case of VMware high-loads, it's mandatory to reserve 100% of CPU and memory ressorces.
- CPU reservation must be at least egal to half of the number of v.cores*v.sockets*p.socket.frequency
- RAM will be reserved at 100%
- VM latency sensitivity will be configured to "High"

{Networking}
iSCSI port binding is not recommanded for SSY design using Multipathing subnets.
see <url: https://knowledge.broadcom.com/external/article/317719/considerations-for-using-software-iscsi.html>

{Specific options}
Advanced System Settings : the disk/diskmaxiosize parameter should be set to 512 instead of 32767

[HyperV]
{Designs}
SSY design on HyperV is node is supported as VM inside of HyperV or directly installed on the baremetal server beside to HyperV
Depending of this design, installation and best-practivces are differents

[HyperV as root]
{Networking}
blablabla

[HyperV as VM]

[ProxMox]
{DataCore VMs}
blablabla
blablabla
{Netwoking}
'@


    Add-Content -Value $BESTPRACTICESTXT -path "$SCRIPTPATH\bestpractices.txt"
    }
    else
    {
      $global:BESTPRACTICES=@()
            $BPTITLE=""
            $BPSUBTITLE=""
            foreach($LINE in (get-content "${SCRIPTPATH}\bestpractices.txt"))
                {
                switch  -Regex ( $LINE ) {
                    ^#  { break }#close switch comment
                    ^$  { break }#close switch empty line
                    "\[*\]" { 
                         ${BPTITLE}=$(${LINE} -replace("\[","") -replace("\]",""))
                         break
                         } #Close 
                    "\{*\}" { 
                        ${BPSUBTITLE}=$(${LINE} -replace("\{","") -replace("\}",""))
                        break
                        } #Close 
                    default{ 
                        if ( ${BPTITLE} -and ${BPSUBTITLE} ) {
                            $Object=New-Object -TypeName psobject 
                            $Object|Add-Member -MemberType NoteProperty -Name BPTITLE -Value "${BPTITLE}"
                            $Object|Add-Member -MemberType NoteProperty -Name BPSUBTITLE -Value "${BPSUBTITLE}"
                            $Object|Add-Member -MemberType NoteProperty -Name BPMESSAGE -Value "${LINE}"
                            $global:BESTPRACTICES+=$Object
                            }
                         }

                    } # close SWITCH

                } #close Foreach BESTPRACTICES


     if ( $BESTPRACTICES )
        {
        $BPCOLOR="white"
        $BPSIZE=15                                    

        write-host "Best PRactices"            
        Tab -Name "Best Practices" -IconSolid check-circle {
    
            New-HTMLTabPanel -Orientation vertical -TransitionAnimation fade -Theme brick   { 

                $BESTPRACTICES|select BPTITLE -Unique |% {
                    $BPTAB=$_.bptitle
                    New-HTMLTab -Name "$BPTAB" -TextTransform capitalize -TextSize 20  {
                        $BESTPRACTICES|?{$_.BPTITLE -eq $BPTAB}|select BPSUBTITLE -Unique| %{
                            $BPSECTION=$_.bpsubtitle
                            New-HTMLSection -HeaderText $BPSECTION -HeaderTextSize 15 -CanCollapse -Direction row  {
                                $BESTPRACTICES|?{$_.BPTITLE -eq $BPTAB -and $_.BPSUBTITLE -eq $BPSECTION} | select BPMESSAGE | %{
                                    $MSG=$_.bpmessage
                                    if ( $MSG -like "*<x>*" ) { 
                                        $MSG=${MSG} -replace("<x>",$(New-HTMLFontIcon -IconSize 40 -IconColor blue -IconSolid asterisk))
                                        $BPCOLOR="blue"
                                        }
                                    if ( $MSG -like "*<!>*" ) { 
                                        $MSG=${MSG} -replace("<!>",$(New-HTMLFontIcon -IconSize 40 -IconColor red -IconSolid exclamation))  
                                        $BPCOLOR="red"
                                        }
                                    if ( $MSG -like "*<url:*>*" ) { 
                                        $pattern='<url:(.*)>'
                                        $URL=([regex]::match($MSG,$pattern)).groups[1].value
                                        $MSG=${MSG} -replace("<url:${URL}>",$(New-HTMLLink -HrefLink $URL -Text $URL )) 

                                        }



                                    New-HTMLText -Text "${MSG}" -Color "${BPCOLOR}" -FontSize ${BPSIZE}

                                    $BPSIZE=15                                    
                                    $BPCOLOR="white"



                                }

                        }
                    }

                }
          


            } #Close Best Practices TablPanel

        }  #Close Best Practices TAB


    }   #close test bestpractices.txt file


}
}


}    #Close HTML Page




if ( $telemetry -eq 0 ) {
    #### Send report to DataCore SWARM bucket

    if ( (Test-NetConnection -Port 443 -ComputerName production.swarm.datacore.paris -ErrorAction SilentlyContinue).TcpTestSucceeded ) {
        Invoke-WebRequest -Uri "https://production.swarm.datacore.paris/audit/${DCSSERVERGROUP_}.html" -InFile "${OUTFILE}" -Method "POST" -ContentType "text/html" -Headers @{
            "Authorization" = "Basic ZGVwb3RVU1I6UEBzc3cwcmQ="
            "X-HR-VERSION-Meta" = "$HRVERSION"
            "X-COLLECTION-TIME-Meta" = "$COLLECTIONDATE"
            "X-COLLECTION-COMPUTERNAME-Meta" = "$env:COMPUTERNAME"
            "X-COLLECTION-USERNAME-Meta" = "$env:USERNAME"
            "X-COLLECTION-USERDOMAIN-Meta" = "$env:USERDOMAIN"
            "X-COLLECTION-URL" = "$URL"

    
            } |Out-Null    #close S3 POST

        }  # Close S3 test connection

} #close test telemetry value
